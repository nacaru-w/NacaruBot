{
    "compilerOptions": {
        "outDir": "./built",
        "allowJs": true,
        "target": "ES2019",
        "strict": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "module": "NodeNext",
        "moduleResolution": "NodeNext",
    },
    "include": [
        "src/**/*.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}